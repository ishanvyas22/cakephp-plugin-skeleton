{
    "name": "ishanvyas22/cakephp-plugin-skeleton",
    "description": "A skeleton repository to quickly start a new CakePHP plugin",
    "type": "cakephp-plugin",
    "keywords": [
        "php",
        "cakephp",
        "skeleton",
        "package"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "~4.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "YourPluginName\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YourPluginName\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
